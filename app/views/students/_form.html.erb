<%= form_for(@student, remote: true, :html => { :role => "form" }) do |form| %>
  

    <div class="field">
      <%= form.label :first_name %>
      <%= form.text_field :first_name %>
    </div>

    <div class="field">
      <%= form.label :last_name %>
      <%= form.text_field :last_name %>
    </div>

    <div class="field">
      <%= form.label :age %>
      <%= form.number_field :age %>
    </div>

    <div class="field">
      <%= form.label :education %>
      <%= form.text_field :education %>
    </div>

    <div class="field">
      <%= form.label :cohort_id %>
      <%= form.text_field :cohort_id %>
    </div>

    <div class="field">
      <%= form.label :created_at %>
      <%= form.datetime_select :created_at %>
    </div>

    <div class="field">
      <%= form.label :updated_at %>
      <%= form.datetime_select :updated_at %>
    </div>

    <div class="field">
      <%= form.label :cohort_id %>
      <%= form.text_field :cohort_id %>
    </div>
  </div>

 
    <button class="btn btn-primary">Save</button>
        <div class="actions">
          <%= form.submit %>
        </div>
      <% end %>
  </div>